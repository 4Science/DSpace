          <dc-qualifier>doi</dc-qualifier>
          <parent />
          <label>Herausgeber DOI</label>
          <input-type>onebox</input-type>
          <repeatable>false</repeatable>
          <required />
          <hint>Bitte geben Sie den DOI des 
ursprünglichen Herausgebers an. Beispiel DOI: https://doi.org/10.20366/unimib/unidata/sn196-1.0</hint>
        </field>
