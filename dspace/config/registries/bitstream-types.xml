<dspace-dc-types>

    <dspace-header>
        <title>DSpace Bitstream Schema</title>
        <contributor.author>Andrea Bollini</contributor.author>
    </dspace-header>

    <dc-schema>
        <name>bitstream</name>
        <namespace>http://dspace.org/bitstream</namespace>
    </dc-schema>

    <dc-type>
        <schema>bitstream</schema>
        <element>hide</element>
        <scope_note>Metadata field used to hide a bitstream inside the advanced-attachment layout</scope_note>
    </dc-type>
    
    <dc-type>
        <schema>bitstream</schema>
        <element>viewer</element>
        <qualifier>viewer</qualifier>
        <scope_note>Metadata field used to set the viewer provider for the target bitstream</scope_note>
    </dc-type>

</dspace-dc-types>
