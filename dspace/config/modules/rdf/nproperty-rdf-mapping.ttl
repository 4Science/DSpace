#CRISRP

#education
#Mapping from crisrp to nested entity
:crisRPedu1
  dm:metadataName "crisrp.link.education" ;
  dm:creates [
    dm:subject dm:DSpaceObjectIRI ;
    dm:predicate ex:education ;
	dm:object [
	a dm:ResourceGenerator ;
	dm:pattern "$DSpaceAuthority" ;
    	];
  ] ;
  .

:crisRPedu2
  dm:metadataName "crisrp.education" ;
  dm:creates [
    dm:subject dm:DSpaceObjectIRI ;
    dm:predicate dc:type;
	dm:object [
	a dm:LiteralGenerator ;
	dm:pattern "Test" ;
    	];
  ] ;
  .

:crisRPedu3
  dm:metadataName "crisrp.education.educationenddate" ;
  dm:creates [
    dm:subject dm:DSpaceObjectIRI ;
    dm:predicate schema:endDate ;
    dm:object [
	a dm:LiteralGenerator;
	dm:pattern "$DSpaceValue" ;
	dm:literalType xsd:date ;
]; 
  ] ;
  .

:crisRPedu4
  dm:metadataName "crisrp.education.educationdescription" ;
  dm:creates [
    dm:subject dm:DSpaceObjectIRI ;
    dm:predicate schema:description ;
    dm:object dm:DSpaceValue ;
  ] ;
  .

:crisRPedu5
  dm:metadataName "crisrp.education.educationplace" ;
  dm:creates [
    dm:subject dm:DSpaceObjectIRI ;
    dm:predicate schema:Place ;
    dm:object dm:DSpaceValue ;
  ] ;
  .

:crisRPedu6
  dm:metadataName "crisrp.education.educationstartdate" ;
  dm:creates [
    dm:subject dm:DSpaceObjectIRI ;
    dm:predicate schema:startDate ;
      dm:object [
	a dm:LiteralGenerator;
	dm:pattern "$DSpaceValue" ;
	dm:literalType xsd:date ;
]; 
  ] ;
  .

