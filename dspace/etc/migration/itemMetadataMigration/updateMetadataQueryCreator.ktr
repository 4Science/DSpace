        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>//Script here

var frs = Entity.split(',');

var query = "update metadatavalue set metadata_field_id = q.metadata_field_id from (select mfrn.metadata_field_id from metadatafieldregistry as mfrn inner join metadataschemaregistry as msrn on msrn.metadata_schema_id = mfrn.metadata_schema_id where msrn.short_id = '"
	+ f71 + "' and mfrn.element = '" + f72 + "' and mfrn.qualifier ";
if (f73)
{
	query += "= '" + f73 + "'";
} else
{
	query += "is null";
}
query += ") q where metadata_value_id in (select mdvi.metadata_value_id from collection2item as c2i inner join metadatavalue as mdvc on mdvc.metadata_field_id in (select mfrc.metadata_field_id from metadatafieldregistry as mfrc where mfrc.metadata_schema_id in (select msr.metadata_schema_id from metadataschemaregistry as msr where msr.short_id = 'dspace') and mfrc.element = 'entity' and mfrc.qualifier = 'type') and ";

if(frs.length > 1)
{
	for (var i = 0; i &lt; frs.length; i++)
	{
		if (i == 0)
		{
			query += "(mdvc.text_value = '" + trim(frs[i]) + "'";
		}else
		{
			query += " or mdvc.text_value = '" + trim(frs[i]) + "'";
		}
	}
	query += ") ";
} else {
	query += "mdvc.text_value = '" + trim(Entity) + "' ";
}

query += "and c2i.collection_id = mdvc.dspace_object_id inner join metadatavalue as mdvi on c2i.item_id = mdvi.dspace_object_id	inner join metadatafieldregistry as mfr on mfr.element = '" + f52 + "' and mfr.qualifier ";

if (f53)
{
	query += "= '" + f53 + "'";
} else
{
	query += "is null";
}

query += " and mdvi.metadata_field_id = mfr.metadata_field_id inner join metadataschemaregistry as msr on msr.short_id = '" + f51 + "' and msr.metadata_schema_id = mfr.metadata_schema_id);";

var queryresult = query;

</jsScript_script>
      </jsScript>
